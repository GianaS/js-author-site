{
  "name": "js-author-site",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "codegen": "graphql-codegen --config codegen.yml",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint --ext .ts,.tsx ./src",
    "precommit": "npm run typecheck && npm run lint",
    "serve": "gatsby serve",
    "start-prod": "npm run clean && npm run build && npm run serve",
    "start": "npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "gatsby": "^4.0.2",
    "gatsby-plugin-canonical-urls": "^4.0.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-google-gtag": "^4.0.0",
    "gatsby-plugin-image": "^2.0.0",
    "gatsby-plugin-manifest": "^4.0.0",
    "gatsby-plugin-react-helmet": "^5.0.0",
    "gatsby-plugin-remove-trailing-slashes": "^4.0.0",
    "gatsby-plugin-robots-txt": "^1.6.14",
    "gatsby-plugin-sharp": "^4.0.1",
    "gatsby-plugin-sitemap": "^5.0.0",
    "gatsby-plugin-styled-components": "^5.0.0",
    "gatsby-source-contentful": "^6.0.0",
    "gatsby-source-filesystem": "^4.0.0",
    "gatsby-transformer-sharp": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.2",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/typescript": "^2.3.0",
    "@graphql-codegen/typescript-operations": "^2.2.0",
    "@graphql-codegen/typescript-react-apollo": "^3.2.0",
    "@types/react": "^17.0.33",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.26.1",
    "prettier": "2.1.1",
    "typescript": "^4.4.4"
  }
}
